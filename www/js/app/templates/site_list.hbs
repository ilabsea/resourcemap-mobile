{{#each siteList}}
<li data-id='{{id}}' data-collection-id='{{collection_id}}'data-name='{{name}}'>
 <a href='{{link}}'> {{name}}
  {{#if collectionName}}
  <p class='ui-li-desc'> {{collectionName}} </p>
  {{/if}}
  <p class='ui-li-aside' style='color:blue'> {{date}} </p>
 </a>
</li>
{{/each}}
{{#if hasMoreSites}}
<li class="load-more" data-id="load-more-site-list" id="load-more-site">
 <a href="javascript:void(0)">Load More Sites ...</a>
</li>
{{/if}}
